<%- include('../../partial/mainheadar.ejs') %>
<style>
    .work-image {
    position: relative;
    overflow: hidden;
}

.main-project-image {
    width: 100%;
    height: 300px;
    object-fit: cover;
    transition: transform 0.3s ease;
}

.additional-images {
    position: absolute;
    bottom: 10px;
    right: 10px;
    display: flex;
    gap: 5px;
    flex-wrap: wrap;
    max-width: 150px;
}

.small-image {
    width: 40px;
    height: 40px;
    border-radius: 8px;
    overflow: hidden;
    border: 2px solid white;
    box-shadow: 0 2px 8px rgba(0,0,0,0.3);
}

.small-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.more-images-indicator {
    width: 40px;
    height: 40px;
    background: rgba(0,0,0,0.8);
    color: white;
    border-radius: 8px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 12px;
    font-weight: bold;
    border: 2px solid white;
}

.project-location,
.project-duration {
    font-size: 14px;
    margin-bottom: 8px;
    color: #ccc;
}

.project-location i,
.project-duration i {
    margin-right: 5px;
    color: #007bff;
}

.images-count {
    font-size: 12px;
    color: #007bff;
    margin-top: 10px;
    font-weight: bold;
}

.images-count i {
    margin-right: 5px;
}

.work-card:hover .main-project-image {
    transform: scale(1.05);
}

.work-card:hover .additional-images {
    opacity: 1;
}
</style>
