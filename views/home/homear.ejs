    <!-- Why Choose Us Section -->
    <div class="container-fluid sections why-choose-section mb-5" data-section="لماذا تختارنا؟">
        <div class="row p-3 align-items-center">
            <div class="col-12 col-md-6 slide-left">
                <div class="logo-container fade-in">
                    <img src="https://res.cloudinary.com/dd03x0wle/image/upload/v1748133721/logo_xogb5e.jpg" alt="شعار جالاكسي جروب" class="company-logo">
                </div>
            </div>
            <div class="col-12 col-md-6 slide-right">
    
